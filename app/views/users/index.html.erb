<table>
  <tr>
    <th>User ID</th>
    <th>Name</th>
	<th>Date Created</th>
  </tr>
  <% @users.each do |user| -%>
  <tr>
	<td><%= user.id -%></td>
	<td><%= user.name -%></td>
	<td><%= user.created_at -%></td>
  </tr>
  <% end -%>
</table>

<strong>Workers: <%= @num_workers -%>

<table>
  <tr>
    <th>Job ID</th>
    <th>Handler</th>
	<th>Date Created</th>
  </tr>
  <% @jobs.each do |job| -%>
  <tr>
	<td><%= job.id -%></td>
	<td><%= job.handler -%></td>
	<td><%= job.created_at -%></td>
  </tr>
  <% end -%>
</table>

